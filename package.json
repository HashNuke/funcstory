  "repository": {
    "type": "git",
    "url": "git+https://github.com/HashNuke/funcstory.git"
  },
  "author": "Akash Manohar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HashNuke/funcstory/issues"
  },
  "homepage": "https://github.com/HashNuke/funcstory#readme",
  "dependencies": {
    "ts-morph": "^20.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
