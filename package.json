{
  "name": "funcstory",
  "version": "1.1.0",
  "description": "Create LLM friendly reports of function calls in your typescript project",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "funcstory": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "typescript",
    "function",
    "tracer",
    "analysis",
    "ast",
    "llm",
    "code-analysis"
